{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "web-core/**/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "web-core/**/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "universal-file/**",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "online-shop-system/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web-core/index-folder/index.html"
    },
    {
      "src": "/about",
      "dest": "/web-core/about-folder/about.html"
    },
    {
      "src": "/products",
      "dest": "/web-core/products-folder/products.html"
    },
    {
      "src": "/contact",
      "dest": "/web-core/contact-folder/contact.html"
    },
    {
      "src": "/stores",
      "dest": "/web-core/stores-folder/stores.html"
    },
    {
      "src": "/kiosk",
      "dest": "/web-core/kiosk-folder/kiosk.html"
    },
    {
      "src": "/member",
      "dest": "/web-core/member-folder/member.html"
    },
    {
      "src": "/news",
      "dest": "/web-core/news-folder/news.html"
    },
    {
      "src": "/policy",
      "dest": "/web-core/policy-folder/policy.html"
    },
    {
      "src": "/cart",
      "dest": "/online-shop-system/cart.html"
    },
    {
      "src": "/universal-file/nav.html",
      "dest": "/universal-file/nav.html"
    },
    {
      "src": "/universal-file/footer.html",
      "dest": "/universal-file/footer.html"
    },
    {
      "src": "/universal-file/nav.css",
      "dest": "/universal-file/nav.css"
    },
    {
      "src": "/universal-file/footer.css",
      "dest": "/universal-file/footer.css"
    },
    {
      "src": "/universal-file/allpage.js",
      "dest": "/universal-file/allpage.js"
    },
    {
      "src": "/universal-file/script.js",
      "dest": "/universal-file/script.js"
    }
  ]
}
